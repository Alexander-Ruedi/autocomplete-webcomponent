{
  "navigationFallback": {
    "rewrite": "index.html",
    "exclude": [
      "/images/*.{png,jpg,gif}",
      "/css/*"
    ]
  },
  "routes": [
    {
      "route": "/assets/*.css",
      "headers": {
        "Content-Type": "text/css;charset=utf-8"
      }
    }
  ],
  "mimeTypes": {
    ".css": "text/css;charset=utf-8"
  }
}
